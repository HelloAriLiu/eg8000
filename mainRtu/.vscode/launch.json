    "version": "0.2.0",
    "configurations": [
        {
            "name": "zh_node.app",/* 程序名称 */
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/zh_node.app",/* 程序路径 */
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
            ],
            "miDebuggerPath":"gdb",
            "miDebuggerServerAddress": "192.168.0.51:200"/* 远程 gdbserver 服务器地址  板子运行：gdbserver 192.168.0.138:200 zh_node.app */
        }
    ]
}